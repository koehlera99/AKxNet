<Window x:Class="TCS.Server.ServerInterface"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TCS.Server"
        mc:Ignorable="d"
        Title="ServerInterface" Height="212" Width="359" Closing="Window_Closing">
    <Grid>
        <Button x:Name="btnStartServer" Content="Start Server" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="100" Height="45" Click="Button_Click"/>
        <Button x:Name="btnStopServer" Content="Stop Server" HorizontalAlignment="Left" Height="45" Margin="10,60,0,0" VerticalAlignment="Top" Width="100" Click="btnStopServer_Click" IsEnabled="False"/>
        <Button x:Name="btnCreateClient" Content="Create Client" HorizontalAlignment="Left" Height="45" Margin="115,10,0,0" VerticalAlignment="Top" Width="100" Click="btnCreateClient_Click"/>
        <Button x:Name="btnWSDL" Content="Load WSDL" HorizontalAlignment="Left" Height="45" Margin="115,60,0,0" VerticalAlignment="Top" Width="100" Click="btnWSDL_Click"/>

    </Grid>
</Window>
